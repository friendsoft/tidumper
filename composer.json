{
    "name": "friendsoft/tidumper",
    "description": "Dump audio meta data from and to tango.info.",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Tim Quellmalz",
            "email": "tim.quellmalz@webentwickler.cc"
        }
    ],
    "require": {
        "guzzle/guzzle": "dev-master",
        "pimple/pimple": "1.0.x-dev",
        "symfony/console": "2.2.x-dev",
        "symfony/css-selector": "2.2.x-dev",
        "symfony/dom-crawler": "2.2.x-dev",
        "symfony/filesystem": "2.3.*@dev",
        "symfony/finder": "2.2.x-dev",
        "symfony/process": "2.3.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Tidumper": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "./bin/symlink-vendor-docs.sh"
        ],
        "post-update-cmd": [
            "./bin/symlink-vendor-docs.sh"
        ]
    }
}
